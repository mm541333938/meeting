<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.harman.meeting_management.dao.UserRoleDao">

    <resultMap id="" type="com.harman.meeting_management.dto.MeetingResultParam" autoMapping="true">

    </resultMap>
    <select id="getRoleList" resultMap="com.harman.meeting_management.mapper.RoleMapper.BaseResultMap">
        SELECT r.*
        FROM
            user_role ur JOIN role r ON ur.role_id = r.id
        WHERE ur.user_id = #{userId}
   </select>
</mapper>